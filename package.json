  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "@parcel/core": "^2.8.3",
    "@parcel/plugin": "^2.0.0",
    "@types/p5": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "nodemon": "^2.0.20",
    "p5": "^1.5.0",
    "parcel": "^2.8.2",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "prettier": "2.8.3",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  }
